<h1>Listing Tryouts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Comeptition</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @tryout_intents.each do |tryout_intent| %>
      <tr>
        <td><%= tryout_intent.date %></td>
        <td><%= tryout_intent.competition.to_s %></td>
        <td><%= link_to 'Show', tryout_intent %></td>
        <td><%= link_to 'Edit', edit_tryout_intent_path(tryout_intent) %></td>
        <td><%= link_to 'Destroy', tryout_intent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Register for Tryouts', new_tryout_intent_path %>
