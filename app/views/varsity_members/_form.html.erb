<%= simple_form_for(@varsity_member) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :vm_id %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :year, collection: 1..4 %>
    <%= f.input :course %>
    <%= f.input :contact_number %>
    <%= f.input :email_address %>
    <%= f.input :varsity_track %>
    <%= f.input :debater_position %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
