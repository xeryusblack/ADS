<%= simple_form_for(@varsity_member) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.label :ID %>
    <%= f.text_field :id %>
  </div>

  <div class="form-inputs">
    <%= f.input :first_name %>
  </div>

  <div class="form-inputs">
    <%= f.input :last_name %>
  </div>

  <div class="form-inputs">
    <%= f.collection_select :year, 1..4 %>
  </div>

  <div class="form-inputs">
    <%= f.input :course %>
  </div>

  <div class="form-inputs">
    <%= f.input :contact_number %>
  </div>

  <div class="form-inputs">
    <%= f.input :email_address %>
  </div>

  <div class="form-inputs">
    <%= f.input :varsity_track, :as => :select, :collection => VarsityMember::TRACK %>
  </div>

  <div class="form-inputs">
    <%= f.input :debater_position, :as => :select, :collection => VarsityMember::POSITION %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
