<h1>Listing Varsity Members</h1>
<div class="responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Year</th>
        <th>Course</th>
        <th>Contact Number</th>
        <th>E-mail Address</th>
        <th>Varsity Track</th>
        <th>Debater Position</th>
        <th>Total Debt</th>
        <th>Total Acquired Quota Points</th>
        <th colspan="10"></th>
      </tr>
    </thead>

    <tbody>
      <% @varsity_members.each do |varsity_member| %>
        <tr>
          <td><%= varsity_member.vm_id %></td>
          <td><%= varsity_member.first_name %></td>
          <td><%= varsity_member.last_name %></td>
          <td><%= varsity_member.year %></td>
          <td><%= varsity_member.course %></td>
          <td><%= varsity_member.contact_number %></td>
          <td><%= varsity_member.email_address %></td>
          <td><%= varsity_member.varsity_track %></td>
          <td><%= varsity_member.debater_position %></td>
          <td><%= varsity_member.total_debt %></td>
          <td><%= varsity_member.total_acquired_quota_points %></td>
          <!-- <td><button class="btn btn-default" type="submit"><%= link_to 'Show', varsity_member %></button></td> -->
          <td><button class="btn btn-default" type="submit"><%= link_to 'Edit', edit_varsity_member_path(varsity_member) %></button></td>
          <td><button class="btn btn-default" type="submit"><%= link_to 'Delete', varsity_member, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>

<button class="btn btn-default" type="submit"><%= link_to 'New Varsity Member', new_varsity_member_path %></button>

<% @competition = Competition.find_by(status: "Ongoing") %>

<% if @competition != nil %>
<%= button_to "End Training Session", edit_competition_path(@competition.id), method: :get %>
<% end %>